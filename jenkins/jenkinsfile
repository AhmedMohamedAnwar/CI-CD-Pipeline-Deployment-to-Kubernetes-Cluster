pipeline{
  agent any
  stages {
    stage ('Checkout Github'){
        steps {
            git branch: 'main',
                credentialsId: 'Github',
                url: 'https://github.com/AhmedMohamedAnwar/CI-CD-Pipeline-Deployment-to-Kubernetes-Cluster'
        }
    }
    stage ('Testing'){
        when{
            anyOf{
                branch 'test'
                branch 'main'
            }
        }
        steps{  
                echo "Testing"
        }
    }
  }
}